<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App5.Page3"
             Title="Page1">
    <ContentPage.Content>
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="50"></RowDefinition>
            </Grid.RowDefinitions>
            <Button Grid.Row="0" Grid.Column="0" Text="Giriş" Clicked="Button_Clicked"></Button>
            <Button Grid.Row="0" Grid.Column="1" Text="Kayıt" Clicked="Button_Clicked_1"></Button>
            <Image Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="2" Source="logo.png"></Image>
            <Image Grid.Row="2" Grid.Column="0" Grid.ColumnSpan="2" Source="icon.png"></Image>
        </Grid>
    </ContentPage.Content>
</ContentPage>